import mongoose from "mongoose";
const doctorSchema = new mongoose.Schema(
  {
    name:{
      type: String,
      required: true
    },
    salary:{
      type: String,
      required: true
    },
    qualifications:{
      type: String,
      required:true
    },
    experienceInYears:{
      type: Number,
      default: 0
    },
    worksInHospitals: [ // doctor work in different hospitals
      {
        type: mongoose.Schema.Types.ObjectId,
        ref: "Hospital",
      },
    ],
  }, {timestamp: true});

export const Doctor = mongoose.model("Doctor",doctorSchema);
